{
	"name": "misskey",
	"version": "2025.1.0-beta.2",
	"codename": "nasubi",
	"repository": {
		"type": "git",
		"url": "https://github.com/misskey-dev/misskey.git"
	},
	"workspaces": [
		"packages/frontend-shared",
		"packages/frontend",
		"packages/frontend-embed",
		"packages/backend",
		"packages/sw",
		"packages/misskey-js",
		"packages/misskey-reversi",
		"packages/misskey-bubble-game"
	],
	"private": true,
	"scripts": {
		"build-pre": "bun ./scripts/build-pre.js",
		"build-assets": "bun ./scripts/build-assets.mjs",
		"build": "bun build-pre && bun run --filter '*' build && bun build-assets",
		"build-storybook": "bun --filter frontend build-storybook",
		"build-misskey-js-with-types": "pnpm build-pre && pnpm --filter backend... --filter=!misskey-js build && pnpm --filter backend generate-api-json --no-build && ncp packages/backend/built/api.json packages/misskey-js/generator/api.json && pnpm --filter misskey-js update-autogen-code && pnpm --filter misskey-js build && pnpm --filter misskey-js api",
		"start": "bun check:connect && cd packages/backend && bun ./built/src/entry.ts",
		"start:test": "cd packages/backend && cross-env NODE_ENV=test bun ./src/boot/entry.ts",
		"init": "pnpm migrate",
		"migrate": "cd packages/backend && bun migrate",
		"revert": "cd packages/backend && bun revert",
		"check:connect": "cd packages/backend && bun check:connect",
		"migrateandstart": "bun migrate && bun start",
		"watch": "bun dev",
		"dev": "bun scripts/dev.mjs",
		"lint": "bun -r lint",
		"cy:open": "bun cypress open --browser --e2e --config-file=cypress.config.ts",
		"cy:run": "bun cypress run",
		"e2e": "bun start-server-and-test start:test http://localhost:61812 cy:run",
		"e2e-dev-container": "cp ./.config/cypress-devcontainer.yml ./.config/test.yml && bun start-server-and-test start:test http://localhost:61812 cy:run",
		"jest": "cd packages/backend && bun jest",
		"jest-and-coverage": "cd packages/backend && bun jest-and-coverage",
		"test": "bun -r test",
		"test-and-coverage": "bun -r test-and-coverage",
		"clean": "bun ./scripts/clean.js",
		"clean-all": "bun ./scripts/clean-all.js",
		"cleanall": "bun clean-all"
	},
	"resolutions": {
		"chokidar": "3.5.3",
		"lodash": "4.17.21"
	},
	"dependencies": {
		"cssnano": "6.1.2",
		"execa": "8.0.1",
		"fast-glob": "3.3.2",
		"ignore-walk": "6.0.5",
		"js-yaml": "4.1.0",
		"postcss": "8.4.49",
		"tar": "6.2.1",
		"terser": "5.36.0",
		"typescript": "5.6.3",
		"esbuild": "0.24.0",
		"glob": "11.0.0"
	},
	"devDependencies": {
		"@misskey-dev/eslint-plugin": "2.0.3",
		"@types/node": "22.9.0",
		"@types/bun": "1.1.18",
		"@typescript-eslint/eslint-plugin": "7.17.0",
		"@typescript-eslint/parser": "7.17.0",
		"cross-env": "7.0.3",
		"cypress": "13.15.2",
		"eslint": "9.14.0",
		"globals": "15.12.0",
		"ncp": "2.0.0",
		"start-server-and-test": "2.0.8"
	},
	"optionalDependencies": {
		"@tensorflow/tfjs-core": "4.4.0"
	},
	"trustedDependencies": [
		"cypress",
		"esbuild",
		"@tensorflow/tfjs-core",
		"@tensorflow/tfjs-node",
		"bufferutil",
		"utf-8-validate",
		"@nestjs/core",
		"@sentry/profiling-node",
		"@swc/core",
		"sharp",
		"msw",
		"core-js",
		"vue-demi",
		"v-code-diff",
		"msgpackr-extract"
	]
}
